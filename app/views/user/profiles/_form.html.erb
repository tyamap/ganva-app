<%= markup do |m|
  p = UserFormPresenter.new(f, self)
  m << p.notes
  m << p.text_field_block(:name, "名前", required: true)
  m << p.text_field_block(:experience, "経歴")
  m << p.text_field_block(:frequency, "頻度")
  m << p.text_field_block(:level, "レベル")
  m << p.drop_down_collection_block(:gym_id, 'マイジム', @gyms, :id, :name, @mygym_id)
  m << p.text_field_block(:introduction, "ひとこと")
end %>