<%= markup do |m|
  p = UserFormPresenter.new(f, self)
  m << p.notes
  p.with_options(required: true) do |q|
    m << q.text_field_block(:name, "名前")
    m << q.text_field_block(:experience, "経歴")
    m << q.text_field_block(:frequency, "頻度")
    m << q.text_field_block(:level, "レベル")
    m << q.text_field_block(:introduction, "ひとこと")
  end
end %>