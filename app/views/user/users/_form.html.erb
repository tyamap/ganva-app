<%= markup do |m|
  p = UserFormPresenter.new(f, self)
  m << p.notes
  p.with_options(required: true) do |q|
    m << q.text_field_block(:email, "メールアドレス", size: 32)
    m << q.password_field_block(:password, "パスワード", size: 32)
    m << q.password_field_block(:password_confirmation, "パスワード（確認）")
    m << q.text_field_block(:uid, "ID", size: 16)
  end
end %>