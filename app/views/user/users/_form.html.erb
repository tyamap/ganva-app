<%= markup do |m|
  p = UserFormPresenter.new(f, self)
  m << p.notes
  p.with_options(required: true, class: 'form-control') do |q|
    m << q.email_field_block(:email, "メールアドレス")
    m << q.password_field_block(:password, "パスワード")
    m << q.password_field_block(:password_confirmation, "パスワード（確認）")
    m << q.text_field_block(:uid, "ID")
  end
end %>